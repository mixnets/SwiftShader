[project]
	description = SwiftShader (go/swiftshader)
[access "refs/heads/*"]
	pushTag = +force group Administrators
	label-Verified = -1..+1 group Contributors
	submit = group Contributors
	push = deny group Administrators
[submit]
	action = cherry pick
[label "Code-Review"]
	copyAllScoresOnTrivialRebase = true
	copyAllScoresIfNoCodeChange = true
[label "Verified"]
	function = MaxWithBlock
	value = -1 Fails
	value =  0 No score
	value = +1 Verified
	defaultValue = 0
	copyAllScoresIfNoCodeChange = true